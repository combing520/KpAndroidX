<?xml version="1.0" encoding="utf-8"?>
<paths>
    <!--对应getFilesDir()。-->
    <files-path name="name" path="path" />
    <!--对应getCacheDir()。-->
    <cache-path name="name" path="path" />
    <!--对应Environment.getExternalStorageDirectory()。-->
    <external-path name="name" path="path" />
    <!--对应getExternalFilesDir()。-->
    <external-files-path name="name" path="path" />
    <!--对应getExternalCacheDir()。-->
    <external-cache-path name="name" path="path" />

    <!--所有路径-->
    <external-path path="." name="external_storage_root" />

    <root-path name="opensdk_root" path=""/>
    <external-files-path name="opensdk_external" path="Images/tmp"/>
    <!-- 友盟微信分享SDK 共享路径-->
    <external-files-path name="umeng_cache" path="umeng_cache/"/>
    <!-- 新浪微博 官方分享SDK 10.10.0共享路径 -->
    <external-files-path name="share_files" path="." />
    <external-path name="sdcard" path="." />
</paths>